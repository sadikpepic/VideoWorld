* {
	margin: 0;
	padding: 0;
	outline: 0;
}

body {
	font-size: 12px;
	line-height: 1.3;
	font-family: Arial, Helvetica, Sans-Serif;
	color: #ccc;
	background: url('../images/body-bg.gif');
}

a {
	color: #e7b038; 6
	text-decoration: none;
	cursor: pointer;
}

a:hover {
	text-decoration: none;
}

a img {
	border: 0;
}

input,textarea,select {
	font-size: 12px;
	font-family: Arial, Helvetica, sans-serif;
}

textarea {
	overflow: auto;
}

.cl {
	display: block;
	height: 0;
	font-size: 0;
	line-height: 0;
	text-indent: -4000px;
	clear: both;
}

.notext {
	font-size: 0;
	line-height: 0;
	text-indent: -4000px;
}

.left,.alignleft {
	float: left;
	display: inline;
}

.right,.alignright {
	float: right;
	display: inline;
}

.text-right {
	text-align: right;
	font-size: 12px;
}

h2 {
	font-size: 15px;
	color: #f2a223;
	font-weight: bold;
}

h3 {
	font-size: 14px;
	color: #fff;
	font-weight: bold;
}

h4 {
	font-size: 14px;
	color: #f3b12b;
	font-weight: bold;
}

.contentBox {
	padding-left: 50px;
	padding-top: 20px;
	min-height: 500px;
	background-color: black;
}

.bestRated {
	margin-top: 10px;
	width: 200px;
	height: 250px;
	border: 1px solid #413e3e;
	background-color: black;
}

.mostViewed {
	width: 200px;
	height: 250px;
	border: 1px solid #413e3e;
	background-color: black;
}

.videoAll {
	background-color: black;
}

.videoFrame {
	float: left;
	position: relative;
}

.results {
	display: none;
	z-index: 999;
	overflow: auto;
}

.delimiter {
	border-bottom: 1px solid white;
}

.floatLeft {
	float: left;
}

.floatRight {
	float: right;
}

.noWhiteSpaceWrap {
	white-space: nowrap;
}

.registerInput {
	width: 200px;
	padding: 8px;
}

.formButton {
	width: 100%;
	height: 30px;
}

.inputText {
	width: 100%;
}

.titleBig {
	font-size: 16px;
	padding-bottom: 20px;
}

.sidebarPosition {
	position: absolute;
}

.videoDescAll {
	position: relative;
	float: right;
	margin: 0px;
	width: 338px;
}

.videoDescTitle {
	padding: 5px;
}

.videoDescContent {
	padding: 5px;
	padding-left: 15px;
}

.filterAll {
	margin: 15px;
}

.togglePic {
	width: 16px;
	height: 16px;
}

.sidebarTitle {
	margin: 5px;
	font-size: 14pt;
}

.loginHeight {
	min-height: 80px;
}

.sidebarBox {
	width: 200px;
	border: 1px solid #413e3e;
	background-color: black;
}

.wishListBox {
	width: 200px;
	min-height: 170px;
	border: 1px solid #413e3e;
	background-color: black;
}

.elements {
	margin: 5px;
}

.radio {
	width: 19px;
	padding-top: 5px;
	margin: 0px;
	display: block;
	clear: left;
	float: left;
	vertical-align: middle;
}

.radioSmall {
	width: 19px;
	padding: 0 5px 0 0;
	display: block;
	clear: left;
	float: left;
	vertical-align: middle;
}

.resPic {
	float: right;
}

.searchTable {
	border: 0px;
	cellspacing: 0px;
	cellpadding: 0px;
	width: 100%;
}

.searchVideoPreview {
	width: 45px;
	height: 60px;
}

.searchVideo {
	width: 45px;
	padding: 2px;
}

.searchVideoTitle {
	padding: 2px;
}

.info,.success,.warning,.error {
	border: 3px solid;
	margin: 10px;
	padding: 15px 15px 15px 50px;
	background-repeat: no-repeat;
	background-position: 10px center;
	float: left;
	width: 895px;
}

.validation {
	border: 3px solid;
	margin: 10px;
	padding: 15px 15px 15px 70px;
	background-repeat: no-repeat;
	background-position: 10px center;
	float: left;
	width: 270px;
}

.registrationSuccess {
	border: 3px solid;
	margin: 10px;
	padding: 15px 15px 15px 70px;
	background-repeat: no-repeat;
	background-position: 10px center;
	float: left;
	width: 270px;
}

.loginError {
	border: 3px solid;
	background-repeat: no-repeat;
	background-position: 10px center;
	width: 180px;
	padding: 5px;
	margin-top: 10px;
}

.info {
	color: #00529B;
	background-color: #BDE5F8;
	background-image: url('../images/icons/info.png');
}

.success {
	color: #4F8A10;
	background-color: #DFF2BF;
	background-image: url('../images/icons/success.png');
}

.warning {
	color: #9F6000;
	background-color: #FEEFB3;
	background-image: url('../images/icons/warning.png');
}

.error {
	color: #D8000C;
	background-color: #FFBABA;
	background-image: url('../images/icons/error.png');
}

.validation {
	color: #D63301;
	background-color: #FFCCBA;
	background-image: url('../images/icons/validation.png');
}

.registrationSuccess {
	color: #4F8A10;
	background-color: #DFF2BF;
	background-image: url('../images/icons/success.png');
}

.loginError {
	color: #D63301;
	background-color: #FFCCBA;
}

#outer {
	width: 1420px;
	margin: 0 auto;
}

#shell {
	width: 985px;
	margin: 0 auto;
}

#header {
	position: relative;
}

h1#logo {
	position: absolute;
	height: 84px;
	left: 0;
	width: 496px;
}

h1#logo a {
	float: left;
	width: 496px;
	height: 84px;
	background: url('../images/logo.png') no-repeat 0 0;
	font-size: 0;
	line-height: 0;
	text-indent: -4000px;
}

#login {
	float: right;
	clear: right;
	padding-top: 10px;
	padding-bottom: 10px;
	min-width: 225px;
}

#login .login-name {
	width: 80px;
}

#login .login-pw {
	width: 80px;
}

#login .login-button {
	width: 50px;
}

#sub-navigation {
	display: block;
	clear: right;
	border-top: 1px dashed #666;
	border-bottom: 1px dashed #666;
	padding: 8px 0;
}

#sub-navigation ul {
	list-style: none;
}

#sub-navigation ul li {
	float: left;
	display: inline;
	padding-right: 23px;
}

#sub-navigation ul li a {
	font-size: 14px;
	font-weight: bold;
	color: #fff;
	line-height: 24px;
	text-decoration: none;
}

#sub-navigation ul li a:hover {
	text-decoration: underline;
}

#search {
	width: 346px;
	margin: 0 0 0 auto;
}

#search label {
	float: left;
	display: inline;
	font-size: 14px;
	font-weight: bold;
	color: #fff;
	line-height: 24px;
	padding-right: 6px;
}

#search .search-field {
	width: 238px;
	border: 1px solid #413e3e;
	background: #000;
	color: #787878;
	padding: 2px 0 2px 2px;
}

#search .search-button {
	font-size: 14px;
	font-weight: bold;
	border: 0;
	background: none;
	color: #fff;
	cursor: pointer;
}

#results {
	position: absolute;
	width: 300px;
	border: 1px solid #413e3e;
	background: #000;
	color: #787878;
	padding: 2px 0 2px 2px;
}

#main {
	border-bottom: 1px dashed #413e3e;
}

.box {
	width: 985px;
	border-bottom: 1px dashed #413e3e;
	padding-bottom: 21px;
}

.box .head {
	width: 985px;
	padding-top: 14px;
	padding-bottom: 11px;
}

.box .head h2 {
	float: left;
	display: inline;
}

.box .movie {
	width: 152px;
	float: left;
	padding-right: 6px;
	padding-left: 6px;
}

.movie-image {
	float: left;
	width: 152px;
	height: 214px;
	position: relative;
}

.movie-image img {
	width: 152px;
	height: 214px;
}

.movie-image a {
	float: left;
	display: inline;
	width: 152px;
	height: 214px;
	position: relative;
	z-index: 2;
}

.play {
	position: absolute;
	top: 0;
	left: 0;
	width: 152px;
	height: 214px;
	background: url('../images/image-hover.png');
	display: block;
	z-index: 5;
	cursor: pointer;
	display: none;
}

.movie span.name {
	font-weight: bold;
	color: #fff;
	font-size: 14px;
	text-align: center;
	padding-top: 160px;
	display: block;
}

.box .last {
	padding: 10px;
}

.rating {
	float: left;
	width: 152px;
	padding-top: 10px;
	padding-bottom: 15px;
}

.rating p {
	float: left;
	font-size: 10px;
	color: #fff;
	font-weight: bold;
}

.rating .stars0 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/0_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars05 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/0.5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars1 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/1_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars15 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/1.5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars2 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/2_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars25 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/2.5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars3 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/3_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars35 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/3.5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars4 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/4_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars45 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/4.5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.rating .stars5 {
	float: left;
	width: 60px;
	height: 11px;
	background: url('../images/Rating/5_stars.png') no-repeat 0 0;
	margin-left: 2px;
}

.content {
	padding-bottom: 28px;
}

.content .date {
	font-size: 10px;
	color: #fff;
}

.content img {
	float: left;
	width: 68px;
	padding-right: 8px;
}

.content p {
	font-size: 13px;
	color: #fff;
	line-height: 16px;
}

.content a {
	font-size: 11px;
}

#footer {
	padding: 15px 0 0 0;
}

#footer p {
	text-align: center;
	font-size: 10px;
	padding-bottom: 11px;
}

#footer a {
	color: #9c9c9c;
	font-size: 10px;
}

#footer a:hover {
	color: #e44400;
	text-decoration: none;
}

#footer a:hover.designby {
	color: #9c9c9c;
	text-decoration: none;
}

.wishList-active {
	background-color: #787878;
}

th {
	padding: 8px;
	width: 200px;
}

td {
	padding: 8px;
	width: 200px;
}

